journal = {Mammalian Biology},
keywords = {Behavior,Big cats,Camera trapping,Carnivores,Trap avoidance},
month = {jan},
pages = {26--30},
publisher = {Elsevier GmbH},
title = {{Do female jaguars (Panthera onca Linnaeus, 1758) deliberately avoid camera traps?}},
volume = {88},
year = {2018}
}
