issn = {0012-9658},
journal = {Ecology},
keywords = {PanTHERIA,behavior,biogeography,body size,climatic variables,comparative analyses,conservation,ecology,geographic range,life history,mammals,population density},
month = {sep},
number = {9},
pages = {2648--2648},
publisher = {Wiley-Blackwell},
title = {{PanTHERIA: a species-level database of life history, ecology, and geography of extant and recently extinct mammals}},
