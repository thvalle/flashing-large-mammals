doi = {10.1016/j.cell.2009.01.052},
file = {:C$\backslash$:/Users/Nora.Familie-PC/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Solovei et al. - 2009 - Nuclear Architecture of Rod Photoreceptor Cells Adapts to Vision in Mammalian Evolution.pdf:pdf},
