journal = {Social Psychological and Personality Science},
keywords = {ROPE,TOST,equivalence testing,null hypothesis significance testing,power analysis,research methods},
mendeley-tags = {ROPE,TOST},
number = {4},
pages = {355--362},
pmid = {28736600},
title = {{Equivalence Tests: A Practical Primer for t Tests, Correlations, and Meta-Analyses}},
volume = {8},
