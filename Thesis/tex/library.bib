isbn = {9788242628466},
keywords = {Eurasian lynx,Lynx lynx,Norway,camera trap,method development,monitoring},
pages = {54},
publisher = {NINA},
title = {{Bruk av viltkamera i overv{\aa}king av gaupe - et pilotstudie i tre omr{\aa}der p{\aa} {\O}stlandet - NINA Rapport 1216}},
url = {https://brage.nina.no/nina-xmlui/handle/11250/2368722},
year = {2015}
}
