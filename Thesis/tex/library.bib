issn = {0022-541X},
journal = {Journal of Wildlife Management},
keywords = {1999,activity pattern,been used in wildlife,camera,canis latrans,census,coyote,cutler and swann,infrared-triggered,photograph,remote photography methods have,sampling,studies to address a,studying behaviors that are,they are useful for,too,variety of questions},
