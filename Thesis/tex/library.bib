issn = {00144835},
journal = {Experimental Eye Research},
keywords = {colorless condensing lens,dog,electron microscopy,histology,indirect ophthalmoscope,light,photoreceptors,retinal degeneration,retinal pigment epithelium,tapetum,yellow-tinted condensing lens},
month = {jul},
number = {1},
